package com.edu;

import java.util.Arrays;

public class OdenamientoBurbuja {

	public static void main(String[] args) {
		int[] i = new int[5];
		i[0] = 78;
		i[1] = 95;
		i[2] = 65;
		i[3] = 99;
		i[4] = 24;
		burbuja( i );
	}

	public static void burbuja(int[] A) {
        int i, j, aux;
        for (i = 0; i < A.length - 1; i++) {
            for (j = 0; j < A.length - i - 1; j++) {
                if (A[j + 1] < A[j]) {
                    aux = A[j + 1];
                    A[j + 1] = A[j];
                    A[j] = aux;
                    for(int a: A) System.out.print(a + " ");
                    System.out.println();
                }
            }
        }
}
	
}
