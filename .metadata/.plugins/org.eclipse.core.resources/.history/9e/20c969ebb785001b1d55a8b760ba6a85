package com.edu;

import java.util.ArrayList;
import java.util.List;

public class MapCustom {

	private String key;
	private String value;
	
	private List<MapCustom> lista = new ArrayList<>();
	
	public MapCustom( String key, String value ){
		this.key = key;
		this.value = value;
	}
	
	protected void add( MapCustom o ) {
		if ( lista.contains(o) ) {
			lista.remove(o);
		}
		
		lista.add( o );
		
	}
	
	
	
	protected MapCustom get( String key ) {
		int index = lista.indexOf(new MapCustom(key, "") );
		return lista.get(index);
	}
	
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	
	
	
}
