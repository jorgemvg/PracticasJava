package com.edu;

public class ClaseEnum {

	enum Season{
		SPRING, SUMMER, AUTUMN, WINTER
	}
	
	public static void metodo () {
		Season season = Season.SPRING;
		
		for( Season s : Season.values() ) {
			System.out.print( s + "," );
		}
	}
	
	public static void main ( String[] args ) {
		
		metodo();
		
	}
	
}
